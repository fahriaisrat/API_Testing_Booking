							"\r",
							"const moment = require(\"moment\")\r",
							"const today = moment()\r",
							"console.log(today.format('YYYY-MM-DD'))\r",
							"pm.environment.set(\"checkin\", today.format('YYYY-MM-DD'))\r",
							"\r",
